<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/FrameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layoutDirection="ltr">

    <FrameLayout
        android:id="@+id/drag_item"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#E6EAEF"
                    android:layoutDirection="rtl"
                    android:orientation="vertical">

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <FrameLayout
                            android:id="@+id/remote_video_view_container"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"/>
                        <ImageView
                            android:id="@+id/big_filterViewer"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:scaleType="centerCrop"
                            android:src="@drawable/f1" />
                    </FrameLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:background="@color/white"
                            android:visibility="gone"
                            android:weightSum="90">

                            <FrameLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginStart="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginEnd="5dp"
                                android:layout_marginBottom="5dp"
                                android:layout_weight="30">

                                <mehdi.sakout.fancybuttons.FancyButton xmlns:fancy="http://schemas.android.com/apk/res-auto"
                                    android:id="@+id/btn_spotify"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="center"
                                    android:gravity="center"
                                    android:onClick="next"
                                    android:paddingLeft="20dp"
                                    android:paddingTop="10dp"
                                    android:paddingRight="20dp"
                                    android:paddingBottom="10dp"
                                    fancy:fb_borderColor="@color/colorPrimary"
                                    fancy:fb_borderWidth="1dp"
                                    fancy:fb_defaultColor="@color/white"
                                    fancy:fb_radius="6dp"
                                    fancy:fb_text=" "
                                    fancy:fb_textColor="@color/white" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="center"
                                    android:layout_weight="30"
                                    android:gravity="center">

                                    <TextView
                                        android:id="@+id/GemsViewer"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:layout_marginEnd="5dp"
                                        android:fontFamily="@font/arb_1"
                                        android:gravity="center"
                                        android:text="..."
                                        android:textColor="@color/colorPrimary"
                                        android:textSize="9sp" />

                                    <ImageView
                                        android:layout_width="20dp"
                                        android:layout_height="20dp"
                                        android:layout_gravity="center"
                                        android:gravity="center"
                                        android:scaleType="fitCenter"
                                        android:src="@drawable/diamond" />
                                </LinearLayout>
                            </FrameLayout>

                            <FrameLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginTop="5dp"
                                android:layout_marginBottom="5dp"
                                android:layout_weight="30">

                                <mehdi.sakout.fancybuttons.FancyButton xmlns:fancy="http://schemas.android.com/apk/res-auto"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="center"
                                    android:gravity="center"
                                    android:onClick="next"
                                    android:paddingLeft="20dp"
                                    android:paddingTop="10dp"
                                    android:paddingRight="20dp"
                                    android:paddingBottom="10dp"
                                    fancy:fb_borderColor="#4CAF50"
                                    fancy:fb_borderWidth="1dp"
                                    fancy:fb_defaultColor="@color/white"
                                    fancy:fb_radius="6dp"
                                    fancy:fb_text=" "
                                    fancy:fb_textColor="@color/white" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="center"
                                    android:layout_weight="30"
                                    android:gravity="center">

                                    <TextView
                                        android:id="@+id/CountryPreferencesViewer"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:layout_marginEnd="5dp"
                                        android:fontFamily="@font/arb_1"
                                        android:gravity="center"
                                        android:text="..."
                                        android:textColor="#393939"
                                        android:textSize="9sp" />

                                    <ImageView
                                        android:id="@+id/imageCountryPreferencesViewer"
                                        android:layout_width="20dp"
                                        android:layout_height="20dp"
                                        android:layout_gravity="center"
                                        android:gravity="center"
                                        android:scaleType="fitCenter"
                                        android:src="@drawable/world" />
                                </LinearLayout>
                            </FrameLayout>

                            <FrameLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginStart="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginEnd="5dp"
                                android:layout_marginBottom="5dp"
                                android:layout_weight="30">

                                <mehdi.sakout.fancybuttons.FancyButton xmlns:fancy="http://schemas.android.com/apk/res-auto"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="center"
                                    android:gravity="center"
                                    android:onClick="next"
                                    android:paddingLeft="20dp"
                                    android:paddingTop="10dp"
                                    android:paddingRight="20dp"
                                    android:paddingBottom="10dp"
                                    fancy:fb_borderColor="#4CAF50"
                                    fancy:fb_borderWidth="1dp"
                                    fancy:fb_defaultColor="@color/white"
                                    fancy:fb_radius="6dp"
                                    fancy:fb_text=" "
                                    fancy:fb_textColor="@color/white" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="center"
                                    android:layout_weight="30"
                                    android:gravity="center">

                                    <TextView
                                        android:id="@+id/GenderPreferencesViewer"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:layout_marginEnd="5dp"
                                        android:fontFamily="@font/arb_1"
                                        android:gravity="center"
                                        android:text="..."
                                        android:textColor="#393939"
                                        android:textSize="9sp" />

                                    <ImageView
                                        android:id="@+id/imageGenderPreferencesViewer"
                                        android:layout_width="20dp"
                                        android:layout_height="20dp"
                                        android:layout_gravity="center"
                                        android:gravity="center"
                                        android:scaleType="fitCenter"
                                        android:src="@drawable/gender_azar" />
                                </LinearLayout>
                            </FrameLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="end"
                            android:layout_margin="10dp"
                            android:layoutDirection="ltr"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="top"
                                android:orientation="vertical">

                                <FrameLayout
                                    android:layout_width="60dp"
                                    android:layout_height="60dp"
                                    android:layout_gravity="top"
                                    android:orientation="vertical">

                                    <de.hdodenhof.circleimageview.CircleImageView
                                        android:id="@+id/profile_image"
                                        android:layout_width="60dp"
                                        android:layout_height="60dp"
                                        android:layout_gravity="center"
                                        android:scaleType="centerCrop"
                                        android:src="@drawable/loading"
                                        app:civ_border_color="@color/white" />

                                    <ImageView
                                        android:id="@+id/matchPlusGoldenBorder"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:background="@drawable/match_plus_frame_chat"
                                        android:visibility="gone" />
                                </FrameLayout>

                                <ImageView
                                    android:id="@+id/report"
                                    android:layout_width="33dp"
                                    android:layout_height="33dp"
                                    android:layout_marginStart="5dp"
                                    android:layout_marginTop="25dp"
                                    android:scaleType="fitCenter"
                                    android:src="@drawable/block_azar" />

                                <ImageView
                                    android:id="@+id/VIPView"
                                    android:layout_width="47dp"
                                    android:layout_height="40dp"
                                    android:layout_marginTop="20dp"
                                    android:background="@drawable/vip"
                                    android:visibility="gone" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="10dp"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/nameViewer"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/arb_1"
                                    android:gravity="center"
                                    android:text="@string/id_247"
                                    android:textColor="@color/white"
                                    android:textSize="13sp" />

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:id="@+id/flag"
                                        android:layout_width="18dp"
                                        android:layout_height="18dp"
                                        android:layout_gravity="center"
                                        android:layout_marginTop="1dp"
                                        android:gravity="center"
                                        android:scaleType="fitCenter"
                                        android:src="@drawable/flag" />

                                    <TextView
                                        android:id="@+id/countryViewer"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:layout_marginStart="10dp"
                                        android:fontFamily="@font/arb_1"
                                        android:gravity="center"
                                        android:text="....."
                                        android:textColor="@color/white"
                                        android:textSize="13sp" />
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:layout_width="20dp"
                                        android:layout_height="20dp"
                                        android:layout_gravity="center"
                                        android:gravity="center"
                                        android:scaleType="fitCenter"
                                        android:src="@drawable/like_azar"
                                        android:tint="@color/white" />

                                    <TextView
                                        android:id="@+id/likesViewer"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:layout_marginStart="5dp"
                                        android:fontFamily="@font/arb_1"
                                        android:gravity="center"
                                        android:textColor="@color/white"
                                        android:textSize="12sp" />
                                </LinearLayout>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="end"
                    android:layout_marginTop="90dp"
                    android:layout_marginEnd="15dp"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/addFriend"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:scaleType="fitCenter"
                        android:src="@drawable/add_f_bold"
                        android:visibility="gone" />

                    <ImageView
                        android:id="@+id/like"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_marginTop="15dp"
                        android:scaleType="fitCenter"
                        android:src="@drawable/like_azar_bold" />

                    <ImageView
                        android:id="@+id/so_like"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginTop="15dp"
                        android:scaleType="fitCenter"
                        android:src="@drawable/heart_azar" />
                </LinearLayout>
            </FrameLayout>
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/filters"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:orientation="horizontal">

                <FrameLayout
                    android:layout_width="100dp"
                    android:layout_height="180dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp">

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <FrameLayout
                            android:id="@+id/localPreview"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent" />

                        <ImageView
                            android:id="@+id/small_filterViewer"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:scaleType="centerCrop"
                            android:src="@drawable/f1" />
                    </FrameLayout>
                </FrameLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:layout_marginStart="5dp"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/switchCamera"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:src="@drawable/rand_flip" />

                    <ImageView
                        android:id="@+id/showEffectsAndFiltersView"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginTop="10dp"
                        android:backgroundTint="@color/white"
                        android:src="@drawable/rand_filters" />

                    <ImageView
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginEnd="10dp"
                        android:layout_marginBottom="20dp"
                        android:onClick="beautyButton"
                        android:scaleType="fitCenter"
                        android:src="@drawable/beauty"
                        android:visibility="gone" />
                </LinearLayout>
            </LinearLayout>

            <include
                layout="@layout/effects_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/otherView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|end"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginEnd="10dp"
                android:onClick="end_call"
                android:src="@drawable/end_call"
                android:tint="@color/colorPrimary"
                app:fabCustomSize="50dp" />
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/voiceCallView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#EDEBED"
            android:visibility="visible">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_above="@id/close"
                android:layout_gravity="top|center_horizontal">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/image2"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:layout_centerInParent="true"
                    android:src="@drawable/loading" />

                <TextView
                    android:id="@+id/name2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/image2"
                    android:layout_gravity="bottom"
                    android:layout_marginTop="20dp"
                    android:fontFamily="@font/arb_1"
                    android:gravity="center"
                    android:text="Loading ..."
                    android:textColor="#9F000000"
                    android:textSize="25dp" />

                <TextView
                    android:id="@+id/counter"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/name2"
                    android:layout_marginTop="10dp"
                    android:fontFamily="@font/arb_1"
                    android:gravity="center"
                    android:text="Connecting"
                    android:textColor="#A5A5A5"
                    android:textSize="25dp" />

                <ImageView
                    android:id="@+id/speaker"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_below="@+id/counter"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"
                    android:onClick="speaker"
                    android:scaleType="fitCenter"
                    android:src="@drawable/speaker_off" />
            </RelativeLayout>

            <LinearLayout
                android:id="@+id/close"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                tools:context=".chat.CallComing">

                <ImageButton
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:layout_marginTop="40dp"
                    android:background="@null"
                    android:onClick="end_call"
                    android:scaleType="fitCenter"
                    android:src="@drawable/phone" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="45dp"
                    android:fontFamily="@font/arb_1"
                    android:gravity="center"
                    android:onClick="end_call"
                    android:text="End Call"
                    android:textColor="#9F000000"
                    android:textSize="15dp" />
            </LinearLayout>
        </RelativeLayout>
    </FrameLayout>
</FrameLayout>
